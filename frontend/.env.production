# 生产环境配置

# 后端API基础URL (请替换为实际的生产环境URL)
REACT_APP_API_URL=https://api.happyday.com/api

# 各微服务URL (生产环境)
REACT_APP_USER_SERVICE_URL=https://api.happyday.com/api
REACT_APP_JOURNAL_SERVICE_URL=https://api.happyday.com/api
REACT_APP_GARDEN_SERVICE_URL=https://api.happyday.com/api
REACT_APP_AI_SERVICE_URL=https://api.happyday.com/api
REACT_APP_MEDITATION_SERVICE_URL=https://api.happyday.com/api

# WebSocket连接URL
REACT_APP_WEBSOCKET_URL=wss://api.happyday.com

# 禁用模拟数据
REACT_APP_USE_MOCK_DATA=false

# 关闭调试模式
REACT_APP_DEBUG=false

# 上传文件的最大尺寸 (MB)
REACT_APP_MAX_FILE_SIZE=10

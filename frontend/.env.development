# 开发环境配置

# 后端API基础URL
REACT_APP_API_URL=http://localhost:8080/api

# 各微服务URL (如果使用微服务架构)
REACT_APP_USER_SERVICE_URL=http://localhost:8080/api
REACT_APP_JOURNAL_SERVICE_URL=http://localhost:8080/api
REACT_APP_GARDEN_SERVICE_URL=http://localhost:8080/api
REACT_APP_AI_SERVICE_URL=http://localhost:8080/api
REACT_APP_MEDITATION_SERVICE_URL=http://localhost:8080/api

# WebSocket连接URL
REACT_APP_WEBSOCKET_URL=ws://localhost:8080

# 是否启用模拟数据 (true/false)
REACT_APP_USE_MOCK_DATA=false

# 调试模式
REACT_APP_DEBUG=true

# 上传文件的最大尺寸 (MB)
REACT_APP_MAX_FILE_SIZE=10
